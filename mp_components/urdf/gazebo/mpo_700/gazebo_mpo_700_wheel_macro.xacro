<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="mpo_700">

  <!--Gazebo Wheel Macro-->
  <xacro:macro name="gazebo_mpo_700_wheel" params="wheel_prefix">

    <gazebo reference="${wheel_prefix}_link">
      <mu1>0.001</mu1>
      <mu2>0.001</mu2>
      <kp>100000.0</kp>
      <kd>1.0</kd>
      <minDepth>0.000</minDepth>
      <fdir1>1 0 0</fdir1>
    </gazebo>
      
  </xacro:macro>

</robot>
