<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="mp_400">

  <!--Gazebo Wheel Macro-->
  <xacro:macro name="gazebo_mp_400_fixed_wheel" params="wheel_prefix">
  
    <gazebo reference="${wheel_prefix}_link">
      <mu1>10</mu1>
      <mu2>10</mu2>
      <kp>100000.0</kp>
      <kd>10.0</kd>
      <minDepth>0.000</minDepth>
      <fdir1>1 0 0</fdir1>
    </gazebo>

  </xacro:macro>

</robot>